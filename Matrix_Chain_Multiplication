#include <bits/stdc++.h>
using namespace std;

int n;
int x[101]; int y[101];
int dp[501][501];

int rec(int l, int r) {
    // pruning
    // base case
    if(l==r){
        return 0;
    }
    // cache check
    if(dp[l][r] != -1){
        return dp[l][r];
    }
    // transition
    int ans = 1e9;
    for(int mid=l;mid<r;mid++){
        ans = min(ans, rec(l,mid)+rec(mid+1,r)+x[l]*y[mid]*y[r]);
    }
    // save and return
    return dp[l][r] = ans;
}

void solve(){
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>x[i]>>y[i];
    }
    memset(dp,-1,sizeof(dp));
    cout <<rec(1,n)<<endl;
}

int main(){
    solve();
}
